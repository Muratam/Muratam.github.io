icon: link(href="!{this.root.innerHTML}" rel="icon")
fa
  script.
    this.root.outerHTML = `<i class="fa fa-${opts.class}"> ${this.root.innerHTML}</i>  `
emimg
  script.
    let size = this.opts.size || 1;
    this.root.outerHTML = `<img src="${this.root.innerHTML}" alt="null"  style="height:${size}em;width:${size}em;">`

a-
  script.
    let split = this.root.innerHTML.split(" ");
    this.root.innerHTML = `<a href="${split[0]}">${split.slice(1).join(" ")}</a>`;
ls
  script.
    for(let child of this.root.children){
      child.outerHTML = `<li>${child.outerHTML}</li>`
    }
    //this.root.outerHTML = `<ul>${this.root.innerHTML}</ul>`

css
  script.
    let rel = "stylesheet"
    if(this.root.innerHTML.endsWith(".less")) rel = "stylesheet/less"
    this.root.outerHTML = `<link href="${this.root.innerHTML}" rel="${rel}">`
js
  script.
    $.getScript(this.root.innerHTML)


